start =
    expression+

validchar = 
	[0-9a-zA-Z_?!+\-=@#$%^&*/.]

whitespace =
	([ \t\r\n] / comment)*

atom =
    chars:validchar+
        { return chars.join(""); }

comment = 
	";;" (!"\n" .)*

expression =
	whitespace a:atom whitespace
		{return a;}
    / whitespace "(" whitespace a:expression+ whitespace ")" whitespace
    	{return a;}